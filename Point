/**
 * New class appointed to snake location (coordinates)
 * 
 * Stephanie Sanchez
 * 5/30/18
 */
public class Point 
{
    private int x;
    private int y;

    public Point(int xCoord, int yCoord)
    {
        x = xCoord;
        y = yCoord;
    }

    public int getX()
    {
        return x;
    }
    
    public int getY()
    {
        return y;
    }
    
    public void setX(int xCoord)
    {
        x=xCoord;
    }
    
    public void setY(int yCoord)
    {
        y=yCoord;
    }
}
